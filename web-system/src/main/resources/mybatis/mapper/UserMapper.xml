<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="indi.study.system.dao.UserDao">

    <select id="findUserList" resultType="indi.study.system.entity.Users">
        select * from users
    </select>

    <select id="findPageUserList" resultType="indi.study.system.entity.Users">
        select * from users
    </select>

    <select id="insertUsers">
        INSERT INTO users (name, age)
        VALUES
        <foreach collection="list" item="item" index="index"
                 separator=",">
            (
            #{item.name}
            ,#{item.age}
            )
        </foreach>
    </select>
</mapper>
